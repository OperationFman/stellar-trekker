# This .gitignore file should be placed at the root of your Unity project directory
#
# Get latest from https://github.com/github/gitignore/blob/main/Unity.gitignore
#
/frontend/[Ll]ibrary/
/frontend/[Tt]emp/
/frontend/[Oo]bj/
/frontend/[Bb]uild/
/frontend/[Bb]uilds/
/frontend/[Ll]ogs/
/frontend/[Uu]ser[Ss]ettings/

# MemoryCaptures can get excessive in size.
# They also could contain extremely sensitive data
/frontend/[Mm]emoryCaptures/

# Recordings can get excessive in size
/frontend/[Rr]ecordings/

# Uncomment this line if you wish to ignore the asset store tools plugin
# /[Aa]ssets/AssetStoreTools*

# Autogenerated Jetbrains Rider plugin
/frontend/[Aa]ssets/Plugins/Editor/JetBrains*

# Visual Studio cache directory
/frontend/.vs/

# Gradle cache directory
/frontend/.gradle/

# Autogenerated VS/MD/Consulo solution and project files
/frontendExportedObj/
/frontend/.consulo/
/frontend/*.csproj
/frontend/*.unityproj
/frontend/*.sln
/frontend/*.suo
/frontend/*.tmp
/frontend/*.user
/frontend/*.userprefs
/frontend/*.pidb
/frontend/*.booproj
/frontend/*.svd
/frontend/*.pdb
/frontend/*.mdb
/frontend/*.opendb
/frontend/*.VC.db

# Unity3D generated meta files
/frontend/*.pidb.meta
/frontend/*.pdb.meta
/frontend/*.mdb.meta

# Unity3D generated file on crash reports
/frontend/sysinfo.txt

# Builds
/frontend/*.apk
/frontend/*.aab
/frontend/*.unitypackage
/frontend/*.app

# Crashlytics generated file
/frontend/crashlytics-build.properties

# Packed Addressables
/frontend/[Aa]ssets/[Aa]ddressable[Aa]ssets[Dd]ata/*/*.bin*

# Temporary auto-generated Android Assets
/frontend/[Aa]ssets/[Ss]treamingAssets/aa.meta
/frontend/[Aa]ssets/[Ss]treamingAssets/aa/*
